        <activity android:name=".DeleteBusDetails" />
        <activity android:name=".UpdateBusDetails" />
        <activity android:name=".AddBusDetails" />
        <activity android:name=".Admin" />
        <activity android:name=".Bus" />
        <activity android:name=".SignUp" />
        <activity android:name=".Seats" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Share" />
        <activity android:name=".MyTicket" />
        <activity android:name=".HomeNav" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>